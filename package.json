{
  "name": "e-commerce-ones-and-zeroes-bn",
  "version": "1.0.0",
  "description": "This is backend part of e-commerce-ones-and-zeroes",
  "main": "server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "nodemon --exec ts-node src/server.ts",
    "test": "jest",
    "seed": "ts-node ./node_modules/.bin/sequelize db:seed:all --env development",
    "migrate": "ts-node ./node_modules/.bin/sequelize db:migrate --env development",
    "unseed": "ts-node ./node_modules/.bin/sequelize db:seed:undo --env development",
    "undo-migrate": "ts-node ./node_modules/.bin/sequelize db:migrate:undo:all --env development"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.17.21",
    "@types/sequelize": "^4.28.20",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "mysql2": "^3.9.4",
    "pg": "^8.11.5",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.2",
    "sequelize-cli": "^6.6.2",
    "sequelize-typescript": "^2.1.6",
    "typescript": "^5.4.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.7",
    "@types/supertest": "^6.0.2",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2"
  }
