{
  "name": "e-commerce-ones-and-zeroes-bn",
  "version": "1.0.0",
  "description": "This is backend part of e-commerce-ones-and-zeroes",
  "main": "server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "nodemon --exec ts-node src/server.ts",
    "test": "jest --coverage",
    "coverage": "jest --coverage",
    "pretest": "tsc",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "seed": "npx ts-node ./node_modules/sequelize-cli/lib/sequelize db:seed:all",
    "migrate": "npx ts-node ./node_modules/sequelize-cli/lib/sequelize db:migrate",
    "unseed": "npx ts-node ./node_modules/sequelize-cli/lib/sequelize db:seed:undo",
    "undo-migrate": "npx ts-node ./node_modules/sequelize-cli/lib/sequelize db:migrate:undo:all",
    "migrate:all": "npm run undo-migrate && npm run migrate && npm run seed",
    "lint": "eslint --fix ",
    "format": "prettier --write .",
    "prepare": "husky",
    "_postinstall": "pinst --disable",
    "prepublishOnly": "pinst --enable"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@sendgrid/mail": "^8.1.3",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.5",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "chai": "^5.1.0",
    "coverage": "^0.4.1",
    "dotenv": "^16.4.5",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^6.9.13",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "express-session": "^1.18.0",
    "pg": "^8.11.5",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.3",
    "sequelize-cli": "^6.6.2",
    "superagent": "^9.0.2",
    "supertest": "^7.0.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "typescript": "^5.4.4",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@babel/preset-env": "^7.24.5",
    "@babel/preset-react": "^7.24.1",
    "@babel/preset-typescript": "^7.24.1",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express-session": "^1.18.0",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/supertest": "^6.0.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^29.7.0",
    "coveralls": "^3.1.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "sequelize-test-helpers": "^1.4.3",
    "sinon-express-mock": "^2.2.1",
    "supertest": "^6.3.4",
    "pinst": "^3.0.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.3",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.1",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run format"
    }
  }
}
