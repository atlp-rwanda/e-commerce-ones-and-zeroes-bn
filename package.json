{
  "name": "e-commerce-ones-and-zeroes-bn",
  "version": "1.0.0",
  "description": "This is backend part of e-commerce-ones-and-zeroes",
  "main": "server.js",
  "scripts": {
    "clean" : "rimraf dist",
    "build": "npm run clean && tsc",
    "start": "node dist/server.js",
    "dev": "nodemon --exec ts-node src/server.ts",
    "test": "jest",
    "seed": "npx ts-node ./node_modules/sequelize-cli/lib/sequelize db:seed:all --env development",
    "migrate": "npx ts-node ./node_modules/sequelize-cli/lib/sequelize db:migrate --env development",
    "unseed": "npx ts-node ./node_modules/sequelize-cli/lib/sequelize db:seed:undo --env development",
    "undo-migrate": "npx ts-node ./node_modules/sequelize-cli/lib/sequelize db:migrate:undo:all --env development",
    "migrate:all": "npm run undo-migrate && npm run migrate && npm run seed"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.5",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "joi": "^17.13.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.5",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.3",
    "sequelize-cli": "^6.6.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "typescript": "^5.4.4",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.24.4",
    "@types/jest": "^29.5.12",
    "@types/supertest": "^6.0.2",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "rimraf": "^5.0.5",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2"
  }
}
